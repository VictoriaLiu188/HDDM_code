geom_violin() +
labs(title = "Violin Plot", x = "Group", y = "Value")
ggsave("violin_", roi_match, ".png", violinplot)
# Create a box plot
boxplot <- ggplot(df_stacked, aes(x = Group, y = Value)) +
geom_boxplot() +
labs(title = "Box Plot", x = "Group", y = "Value")
ggsave("box_", roi_match, ".png", boxplot)
}
}
